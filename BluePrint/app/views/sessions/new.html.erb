<div class="card animated fadeInDown">
  <div class="Sign_Form">
    <div class="login-header">
      <h1>Login</h1>
    </div>
    <div class="form-body">
    	<%= form_tag '/login' do %>
			<p><%= text_field_tag :email, nil, placeholder: "Email" %></p>
			<p><%= password_field_tag :password, nil, placeholder: "Password" %></p>
			<%= submit_tag "Submit", class: 'btn login-btn' %>
		<% end %>
    </div>
  </div>
</div>  